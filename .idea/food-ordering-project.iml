<?xml version="1.0" encoding="UTF-8"?>
<!-- ================================================================ -->
<!-- فایل ماژول اصلی پروژه سیستم سفارش غذا -->
<!-- این فایل تعریف ماژول اصلی پروژه در IntelliJ IDEA است -->
<!-- نویسنده: تیم توسعه -->
<!-- تاریخ آخرین ویرایش: تیر ۱۴۰۴ -->
<!-- نسخه: ۲.۰ - تنظیمات ماژول پیشرفته -->
<!-- ================================================================ -->

<module type="JAVA_MODULE" version="4">
  <!-- ================================================================ -->
  <!-- بخش مدیریت ریشه ماژول جدید -->
  <!-- ================================================================ -->
  <component name="NewModuleRootManager" inherit-compiler-output="true">
    <!-- ================================================================ -->
    <!-- حذف خروجی کامپایلر از index -->
    <!-- ================================================================ -->
    <exclude-output />
    
    <!-- ================================================================ -->
    <!-- محتوای ماژول - مسیر ریشه پروژه -->
    <!-- ================================================================ -->
    <content url="file://$MODULE_DIR$" />
    
    <!-- ================================================================ -->
    <!-- ورودی JDK ارث‌بری شده -->
    <!-- ================================================================ -->
    <orderEntry type="inheritedJdk" />
    
    <!-- ================================================================ -->
    <!-- پوشه منبع برای فایل‌های غیر تست -->
    <!-- ================================================================ -->
    <orderEntry type="sourceFolder" forTests="false" />
  </component>
</module>