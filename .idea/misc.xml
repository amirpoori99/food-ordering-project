<?xml version="1.0" encoding="UTF-8"?>
<!-- ================================================================ -->
<!-- فایل تنظیمات متفرقه پروژه سیستم سفارش غذا -->
<!-- این فایل شامل تنظیمات مختلف پروژه مانند JDK، Maven و ذخیره‌سازی خارجی است -->
<!-- نویسنده: تیم توسعه -->
<!-- تاریخ آخرین ویرایش: تیر ۱۴۰۴ -->
<!-- نسخه: ۲.۰ - تنظیمات متفرقه پیشرفته -->
<!-- ================================================================ -->

<project version="4">
  <!-- ================================================================ -->
  <!-- بخش تنظیمات ذخیره‌سازی خارجی -->
  <!-- ================================================================ -->
  <component name="ExternalStorageConfigurationManager" enabled="true" />
  
  <!-- ================================================================ -->
  <!-- بخش مدیریت پروژه‌های Maven -->
  <!-- ================================================================ -->
  <component name="MavenProjectsManager">
    <!-- ================================================================ -->
    <!-- لیست فایل‌های اصلی Maven -->
    <!-- ================================================================ -->
    <option name="originalFiles">
      <list>
        <!-- ================================================================ -->
        <!-- فایل POM بک‌اند -->
        <!-- ================================================================ -->
        <option value="$PROJECT_DIR$/backend/pom.xml" />
        
        <!-- ================================================================ -->
        <!-- فایل POM فرانت‌اند -->
        <!-- ================================================================ -->
        <option value="$PROJECT_DIR$/frontend-javafx/pom.xml" />
      </list>
    </option>
  </component>
  
  <!-- ================================================================ -->
  <!-- بخش مدیریت ریشه پروژه -->
  <!-- ================================================================ -->
  <component name="ProjectRootManager" version="2" languageLevel="JDK_24" default="true" project-jdk-name="openjdk-24" project-jdk-type="JavaSDK">
    <!-- ================================================================ -->
    <!-- مسیر خروجی پروژه -->
    <!-- ================================================================ -->
    <output url="file://$PROJECT_DIR$/out" />
  </component>
</project>